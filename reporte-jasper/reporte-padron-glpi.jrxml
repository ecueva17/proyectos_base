	<parameter name="FONDO_RENIEC" class="java.lang.String"/>
	<parameter name="LOGO_RENIEC" class="java.lang.String"/>
	<parameter name="CO_DEPARTAMENTO" class="java.lang.String"/>
	<parameter name="CO_PROVINCIA" class="java.lang.String"/>
	<parameter name="CO_DISTRITO" class="java.lang.String"/>
